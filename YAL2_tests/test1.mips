# Début d'un programme
.text
main :
#Initialiser $s7 (registre 7) avec $sp
	move $s7, $sp

# Réserver la place pour 2 variables dans $s7
	add $sp, $sp,-8

#Affectation  b = 19
	li $a0, 19
	sw $a0, -4($s7)

#Affectation  a = 1 + 3
	# Operation : 1 + 3
	li $t0, 1
	li $t1, 3
	add $a0, $t0, $t1
	sw $a0, 0($s7)

#Ecriture de a
	li $v0, 1
	lw $a0, 0($s7)
	syscall

# Fin du programme
end :
	li $v0, 10
	syscall
